#include <stdio.h>
int main (){
    float nota1, nota2, media;
    printf("Digite a nota1: ");
    scanf("%f", &nota1);
    printf("Digite a nota2: ");
    scanf("%f", &nota2);
    
    media = (nota1+nota2)/2;
    
    printf("A media e: %1.f", media);
    if(media>=5){
        printf("aprovado");
    } else{
        printf("reprovado");
    }
    return 0;
}
