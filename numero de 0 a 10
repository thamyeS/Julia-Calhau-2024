#include <stdio.h>
int main (){
    int numero;
    
    for(;;) {
        printf("Digite uma nota de 0 a 10:");
        scanf("%d", &numero);
        
        if(numero <=10) {
            break;
      }
      else
      {
          printf("Valor inválido.\n");
      }
            
    }
    
    printf("Você digitou o número certo. Fim do programa.\n");
    
    return 0;
}
