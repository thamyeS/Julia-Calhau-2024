#include <stdio.h>
int main (){
    int ganhos, gastos, saldo;
    char nome_pessoa[100];
  
    printf("digite nome da pessoa: ");
    scanf("%s",nome_pessoa);
    printf("digite quanto de ganhos que obteve: ");
    scanf("%d",&ganhos);
    printf("digite quanto de gastos que obteve: ");
    scanf("%d",&gastos);
    
    saldo=ganhos-gastos;
    
    printf("o saldo Ã© :%d", saldo);
    if (saldo>=200){
        printf("tenho saldo");
    } else{
        printf("pouco saldo");
    }
    return 0;
}
