#include <stdio.h>

int main() {
    int numero, contador;


    printf("Digite um numero para ver a tabuada: ");
    scanf("%d", &numero);

 
    printf("Tabuada do %d:\n", numero);
    contador = 1;
    while (contador <= 10) {
        printf("%d x %d = %d\n", numero, contador, numero * contador);
        contador++;
    }

    return 0;
}
