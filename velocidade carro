#include <stdio.h>
int main (){
    int velocidade;
    char nome_carro [100];
  
    printf("digite nome do carro: ");
    scanf("%s",nome_carro);
    printf("digite a velocidade do carro: ");
    scanf("%d",&velocidade);
    
    printf("a velocidade e :%d", velocidade);
    if (velocidade>=100){
        printf("rapido");
    } else{
        printf("devagar");
    }
    return 0;
}
